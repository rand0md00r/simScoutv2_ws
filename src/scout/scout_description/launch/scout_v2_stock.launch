
<launch>
    <arg name="robot_namespace" default="/"/>
    <!-- 2.2.1 加载世界urdf模型 -->
    <arg name="urdf_extras" default="$(find scout_description)/urdf/empty.urdf"/>
    <!-- 2.2.2加载机器人xacro模型    combination  scout_v2 -->
    <param name="robot_description" command="$(find xacro)/xacro '$(find scout_description)/urdf/scout_v2.xacro' robot_namespace:=$(arg robot_namespace) urdf_extras:=$(arg urdf_extras)" />
</launch>
